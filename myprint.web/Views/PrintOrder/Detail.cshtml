@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>荣荣打印 | 订单详情</title>
    <!-- 告诉浏览器该页面是自适应布局 -->
    <meta content="wnameth=device-wnameth, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="shortcut icon" href="~/favicon.ico">
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="~/Content/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/bower_components/font-awesome/css/font-awesome.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/Content/bower_components/Ionicons/css/ionicons.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/css/AdminLTE.min.css">
    <!-- AdminLTE 皮肤。
         从 css/skins 目录下选择一个皮肤，以减少负载，而不是下载所有皮肤。 -->
    <link rel="stylesheet" href="~/Content/css/skins/_all-skins.min.css">
</head>
<body class="hold-transition skin-blue layout-top-nav">
    <div class="wrapper">

        <header class="main-header">
            <nav class="navbar navbar-static-top">
                <div class="container">
                    <div class="navbar-header">
                        <a href="/index" class="navbar-brand">荣荣打印</a>
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
                            <i class="fa fa-bars"></i>
                        </button>
                    </div>

                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li><a href="/index">记录打印<span class="sr-only">（当前）</span></a></li>
                            <li class="active"><a href="/printorder">打印统计</a></li>
                            <li><a href="/timingstatistics">定时统计</a></li>
                        </ul>
                    </div>


                    <!-- Navbar Right Menu -->
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">
                            <!-- User Account Menu -->
                            <li class="dropdown user user-menu">
                                <!-- Menu Toggle Button -->
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <!-- The user image in the navbar-->
                                    <img src="~/img/user2-160x160.jpg" class="user-image" alt="用户图像">
                                    <!-- hidden-xs 在小型设备上隐藏用户名，只显示图像。 -->
                                    <span class="hidden-xs"> @Session["userName"] </span>
                                </a>
                                <ul class="dropdown-menu">
                                    <!-- The user image in the menu -->
                                    <li class="user-header">
                                        <img src="~/img/user2-160x160.jpg" class="img-circle" alt="用户图像">
                                        <p>
                                            @Session["userName"]
                                            <small>注册于 @Session["createTime"]</small>
                                        </p>
                                    </li>
                                    <!-- Menu Body -->
                                    <!-- Menu Footer-->
                                    <li class="user-footer">
                                        <div class="pull-left">
                                            <a href="/profile" class="btn btn-default btn-flat">资料</a>
                                        </div>
                                        <div class="pull-right">
                                            <a href="/login/logout" class="btn btn-default btn-flat">退出</a>
                                        </div>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <!-- /.navbar-custom-menu -->
                </div>
            </nav>
        </header>
        <!-- Full Wnameth Column -->
        <div class="content-wrapper">
            <div class="container">
                <!-- Content Header (页眉) -->
                <section class="content-header">
                    <h1>
                        记录打印
                        <small>订单详情</small>
                    </h1>
                </section>

                <!-- Main content -->
                <section class="content">
                    <div class="box box-default">
                       
                        <div class="box-header with-border">
                            <h3 class="box-title">订单详情</h3>
                        </div>
                        <div class="box-body">
                            <form action="/printorder/confirm" method="post">
                                <input type="hidden" value="@Model.id" name="id" />
                                <strong><i class="fa   fa-user margin-r-5"></i>序号</strong>
                                <p class="text-muted">
                                    @Model.id
                                </p>
                                <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>顾客姓名</strong>
                                <p class="text-muted">
                                    @Model.user_name
                                </p>
                                <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>总价</strong>
                                <p class="text-muted">
                                    @Model.total_amount 元
                                </p> <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>完成</strong>
                                <p class="text-muted">
                                    @Model.order_status
                                </p> <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>打印份数</strong>
                                <p class="text-muted">
                                    @Model.prinf_number
                                </p>
                                <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>张数</strong>
                                <p class="text-muted">
                                    @Model.paper_number
                                </p>
                                <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>价格</strong>
                                <p class="text-muted">
                                    @Model.amount 元
                                </p> <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>顾客QQ</strong>
                                <p class="text-muted">
                                    @Model.user_qq
                                </p>
                                <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>顾客微信</strong>
                                <p class="text-muted">
                                    @Model.user_wxchat
                                </p>
                                <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>手机号</strong>
                                <p class="text-muted">
                                    @Model.user_qq
                                </p>
                                <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>文件名</strong>
                                <p class="text-muted">
                                    @Model.print_file_name
                                </p>
                                <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>备注</strong>
                                <p class="text-muted">
                                    @Model.note
                                </p> <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>地址</strong>
                                <p class="text-muted">
                                    @Model.address
                                </p> <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>创建时间</strong>
                                <p class="text-muted">
                                    @Model.create_time
                                </p> <hr>
                                <strong><i class="fa   fa-user margin-r-5"></i>更新时间</strong>
                                <p class="text-muted">
                                    @Model.update_time
                                </p>

                                <div class="box-footer">
                                    <button type="button" class="btn btn-default" onclick="history.go(-1)">返回</button>
                                    <button type="submit" class="btn btn-info pull-right">确认完成</button>
                                </div>
                            </form>

                        </div>
                    </div>
                    <!-- /.box -->
                </section>
                <!-- /.content -->
            </div>
            <!-- /.container -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="container">
                <div class="pull-right hnameden-xs">
                    <b>Version</b> 1.0.0
                </div>
                <strong>Copyright &copy; 2020-2021 <a href="/about">荣荣打印</a>.</strong> All rights
                reserved.
            </div>
            <!-- /.container -->
        </footer>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="~/Content/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/Content/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- SlimScroll -->
    <script src="~/Content/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/Content/bower_components/fastclick/lib/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Scripts/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Scripts/js/demo.js"></script>
</body>
</html>